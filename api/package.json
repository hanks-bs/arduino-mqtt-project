{
  "name": "arduino-mqtt-project-api",
  "version": "1.0.3",
  "main": "index.js",
  "private": true,
  "module": "ESNext",
  "moduleResolution": "node",
  "engines": {
    "node": ">=20.11.0 <23",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "watch": "tsc -w",
    "start": "node dist/src/app.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register ./src/app.ts",
    "harness": "ts-node -r tsconfig-paths/register ./src/scripts/reliabilityHarness.ts",
  "measure": "ts-node -r tsconfig-paths/register ./src/scripts/measurementRunner.ts",
  "docs:research:update": "ts-node -r tsconfig-paths/register ./src/scripts/updateResearchDoc.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "prettier": "prettier 'src/**/*.{js,ts,json}' --check",
    "prettier:fix": "prettier 'src/**/*.{js,ts,json}' --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "docs": "typedoc --options typedoc.json"
  },
  "dependencies": {
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@serialport/parser-readline": "^13.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint": "^9.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "fs-extra": "^11.1.0",
    "helmet": "^6.0.1",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mqtt": "^5.10.3",
    "pidusage": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "serialport": "^13.0.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.0.1",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.10",
    "@types/morgan": "^1.9.4",
    "@types/pidusage": "^2.0.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nodemon": "^2.0.20",
    "prettier": "^3.0.3",
    "supertest": "^7.1.4",
  "socket.io-client": "^4.8.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.1.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.3.3"
  },
  "license": "unlicensed",
  "repository": "https://github.com/hanks-bs/arduino-mqtt-project-api"
}
